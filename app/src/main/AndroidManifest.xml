<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.chpir.android.roster"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.activeandroid.app.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CenterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ParticipantEditorActivity">
        </activity>
        <activity android:name=".RosterActivity">
        </activity>
        <activity android:name=".ResponseViewerActivity">
        </activity>
        <activity android:name=".ParticipantViewerActivity">
        </activity>
        <meta-data android:name="AA_DB_NAME" android:value="RosterDemo.db" />
        <meta-data android:name="AA_DB_VERSION" android:value="1" />
    </application>

</manifest>